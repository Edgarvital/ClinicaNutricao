<p id="notice"><%= notice %></p>

<h1>Consults</h1>

<table>
  <thead>
    <tr>
      <th>Prontuario</th>
      <th>Data</th>
      <th>Paciente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consults.each do |consult| %>
      <tr>
        <td><%= consult.prontuario %></td>
        <td><%= consult.data %></td>
        <td><%= link_to 'Show', paciente_consult_path(@paciente, consult) %></td>
        <td><%= link_to 'Edit Paciente', edit_paciente_consult_path(@paciente, consult) %></td>
        <td><%= link_to 'Destroy', [@paciente, consult], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consult', new_paciente_consult_path(@paciente) %>
