<p id="notice"><%= notice %></p>

<h1>Retornos</h1>

<table>
  <thead>
  <tr>
    <th>Prontuario</th>
    <th>Data</th>
    <th>Estado</th>
    <th>Consult</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @retornos.each do |retorno| %>
    <tr>
      <td><%= retorno.prontuario %></td>
      <td><%= retorno.data %></td>
      <td><%= link_to 'Exibir', consult_retorno_path(@consult, retorno) %></td>
      <% if (nutri_signed_in?) %>
        <td><%= link_to 'Preencher Retorno', edit_consult_retorno_path(@consult, retorno) %></td>
        <td><%= link_to 'Destruir', [@consult, retorno], method: :delete, data: {confirm: 'Are you sure?'} %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>

<br>
<% if (nutri_signed_in?) %>
  <%= link_to 'New Retorno', new_retorno_path %>
<% end %>
